{
  "compilerOptions": {
    "target": "es5",
    "resolveJsonModule": true,
    "sourceMap": true,
    "alwaysStrict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "lib": [
      "es2015", "dom"
    ]
  },
  "exclude": [
    "wwwroot",
    "node_modules"
  ]
}
